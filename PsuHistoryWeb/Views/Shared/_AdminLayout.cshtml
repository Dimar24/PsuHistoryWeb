<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PsuWebApplication</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/adminSite.css" />

    <link rel="stylesheet" href="~/lib/leaflet/dist/css/leaflet.css" />
    <link rel="stylesheet" href="~/lib/leaflet-gesture-handling/dist/css/leaflet-gesture-handling.css">
</head>
<body class="fixed-nav  bg-dark" style="min-width: 400px">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
        <a class="navbar-brand" href="/admin/posts">Панель Администратора</a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse"
                data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false"
                aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
            <ul class="navbar-nav navbar-sidenav" id="exampleAccordion">

                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Burial" asp-action="Burial">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Захоронения</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="TypeBurial" asp-action="TypeBurial">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Тип Захоронения</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Victim" asp-action="Victim">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Погибшие</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="BirthPlace" asp-action="BirthPlace">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Место Рождения</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="ConscriptionPlace" asp-action="ConscriptionPlace">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Место призыва</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="DutyStation" asp-action="DutyStation">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Место службы</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="TypeVictim" asp-action="TypeVictim">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Тип Погибшего</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Admin" asp-action="Report">
                        <i class="fa fa-fw fa-plus"></i>
                        <span class="nav-link-text">Составить Отчет</span>
                    </a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                        <i class="fa fa-fw fa-list"></i>
                        <span class="nav-link-text">Главная сайта</span>
                    </a>
                </li>

                @*<li class="nav-item">
            <a type="submit" name="enter" class="nav-link" asp-area="" asp-controller="Account" asp-action="Logout">
                <i class="fa fa-fw fa-list"></i>
                <span class="nav-link-text">Выход</span>
            </a>
        </li>*@

            </ul>
        </div>
    </nav>
    @RenderBody()
    <script src="~/lib/leaflet/dist/js/leaflet.min.js.js"></script>
    <script src="~/lib/leaflet-gesture-handling/dist/js/leaflet-gesture-handling.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>